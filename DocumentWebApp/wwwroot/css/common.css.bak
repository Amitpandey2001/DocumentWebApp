/*========= File Upload Styling ==========*/
.logoContainer img {
    width: 45px;
    height: 45px;
}

    .logoContainer img:focus {
        color: #495057;
        background-color: #fff;
        border-color: #80bdff;
        outline: 0;
        box-shadow: 0 0 0 0.2rem rgba(0,123,255,.25);
    }

.fileContainer {
    position: relative;
    cursor: pointer;
}

    .fileContainer span {
        overflow: hidden;
        font-weight: bold;
        display: block;
        white-space: nowrap;
        text-overflow: ellipsis;
        cursor: pointer;
    }

    .fileContainer input[type="file"] {
        opacity: 0;
        margin: 0;
        padding: 0;
        width: 100%;
        height: 100%;
        left: 0;
        top: 0;
        position: absolute;
        cursor: pointer;
        color: #495057;
    }
